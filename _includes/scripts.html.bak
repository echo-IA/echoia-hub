<script src="{{ '/assets/js/jquery.min.js' | relative_url }}"></script>

{% if include.dropotron %}
  <script src="{{ '/assets/js/jquery.dropotron.min.js' | relative_url }}"></script>
{% endif %}

<script src="{{ '/assets/js/jquery.scrolly.min.js' | relative_url }}"></script>
<script src="{{ '/assets/js/jquery.scrollex.min.js' | relative_url }}"></script>
<script src="{{ '/assets/js/browser.min.js' | relative_url }}"></script>
<script src="{{ '/assets/js/breakpoints.min.js' | relative_url }}"></script>
<script src="{{ '/assets/js/util.js' | relative_url }}"></script>
<script src="{{ '/assets/js/main.js' | relative_url }}"></script>

{% comment %} Add MathJax here {% endcomment %}
{% if page.mathjax %}
  <script id="MathJax-script" async
          src="https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-mml-chtml.js"></script>
{% endif %}

{% if include.papaparse %}
  <script src="https://cdn.jsdelivr.net/npm/papaparse@5.4.1/papaparse.min.js"></script>
{% endif %}

{% if include.extra %}
  {% assign extras = include.extra | split: ',' %}
  {% for url in extras %}
    {% if url and url != '' %}
      <script src="{{ url | strip }}"></script>
    {% endif %}
  {% endfor %}
{% endif %}
